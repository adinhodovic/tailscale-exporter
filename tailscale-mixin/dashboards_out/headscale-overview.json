{
   "__inputs": [ ],
   "__requires": [ ],
   "annotations": {
      "list": [ ]
   },
   "description": "An overview of Headscale metrics collected by tailscale-exporter. The dashboards were generated using [tailscale-mixin](https://github.com/adinhodovic/tailscale-exporter/tree/main/tailscale-mixin). Open issues and create feature requests in the repository.",
   "editable": false,
   "links": [
      {
         "asDropdown": false,
         "includeVars": false,
         "keepTime": true,
         "tags": [
            "tailscale",
            "tailscale-mixin"
         ],
         "targetBlank": true,
         "title": "Tailscale",
         "type": "dashboards"
      }
   ],
   "panels": [
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "title": "Summary",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Number of Headscale nodes discovered by the exporter.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_nodes_info{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n"
            }
         ],
         "title": "Nodes",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Nodes currently online according to Headscale.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 4,
            "y": 1
         },
         "id": 3,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "sum(\n  headscale_nodes_online{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n"
            }
         ],
         "title": "Online Nodes",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Nodes that Headscale considers offline.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 8,
            "y": 1
         },
         "id": 4,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_nodes_online{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  } == 0\n)\n"
            }
         ],
         "title": "Offline Nodes",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Total number of users known to Headscale.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 12,
            "y": 1
         },
         "id": 5,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_users_info{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n"
            }
         ],
         "title": "Users",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Count of Headscale API keys.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 16,
            "y": 1
         },
         "id": 6,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_apikeys_info{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n"
            }
         ],
         "title": "API Keys",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "API keys that expire within the next 30 days.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 20,
            "y": 1
         },
         "id": 7,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_apikeys_expiration_timestamp{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  } < time() + 30 * 24 * 60 * 60\n)\n"
            }
         ],
         "title": "API Keys <30d",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Count of Headscale pre-authentication keys.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 0,
            "y": 5
         },
         "id": 8,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_preauthkeys_info{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n"
            }
         ],
         "title": "Pre-auth Keys",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Pre-authentication keys that expire within the next 7 days.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 4,
            "y": 5
         },
         "id": 9,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "count(\n  headscale_preauthkeys_expiration_timestamp{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  } < time() + 7 * 24 * 60 * 60\n)\n"
            }
         ],
         "title": "Pre-auth Keys <7d",
         "type": "stat"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Reported database connectivity state from the Headscale health endpoint.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "bool"
            }
         },
         "gridPos": {
            "h": 4,
            "w": 4,
            "x": 8,
            "y": 5
         },
         "id": 10,
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${datasource}"
               },
               "expr": "max(\n  headscale_health_database_connectivity{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n"
            }
         ],
         "title": "Database Connectivity",
         "type": "stat"
      },
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 5
         },
         "id": 11,
         "title": "Nodes",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Distribution of nodes grouped by user.",
         "fieldConfig": {
            "defaults": {
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 6,
            "w": 8,
            "x": 0,
            "y": 6
         },
         "id": 12,
         "options": {
            "displayLabels": [
               "percent"
            ],
            "legend": {
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "values": [
                  "percent"
               ]
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "count(\n  headscale_nodes_info{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n) by (user)\n",
               "instant": true,
               "legendFormat": "{{ user }}"
            }
         ],
         "title": "Nodes by User",
         "type": "piechart"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Distribution of nodes grouped by registration method.",
         "fieldConfig": {
            "defaults": {
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 6,
            "w": 8,
            "x": 8,
            "y": 6
         },
         "id": 13,
         "options": {
            "displayLabels": [
               "percent"
            ],
            "legend": {
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "values": [
                  "percent"
               ]
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "count(\n  headscale_nodes_info{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n) by (register_method)\n",
               "instant": true,
               "legendFormat": "{{ register_method }}"
            }
         ],
         "title": "Nodes by Register Method",
         "type": "piechart"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Breakdown of tags reported per node grouped by category.",
         "fieldConfig": {
            "defaults": {
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 6,
            "w": 8,
            "x": 16,
            "y": 6
         },
         "id": 14,
         "options": {
            "displayLabels": [
               "percent"
            ],
            "legend": {
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "values": [
                  "percent"
               ]
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n  headscale_nodes_tags{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n) by (category)\n",
               "instant": true,
               "legendFormat": "{{ category }}"
            }
         ],
         "title": "Node Tags",
         "type": "piechart"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Total number of nodes that Headscale reports as online.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 12
         },
         "id": 15,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  headscale_nodes_online{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n",
               "legendFormat": "Online"
            }
         ],
         "title": "Nodes Online",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Counts of routes advertised by nodes grouped by state.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10
               },
               "unit": "short"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 12
         },
         "id": 16,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  headscale_nodes_approved_routes{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n",
               "legendFormat": "Approved"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  headscale_nodes_available_routes{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n",
               "legendFormat": "Available"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  headscale_nodes_subnet_routes{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n)\n",
               "legendFormat": "Subnet"
            }
         ],
         "title": "Advertised Routes",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Node metadata reported by Headscale.",
         "fieldConfig": {
            "defaults": {
               "unit": "string"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 10,
            "w": 24,
            "x": 0,
            "y": 20
         },
         "id": 17,
         "options": {
            "footer": {
               "enablePagination": true
            },
            "sortBy": [
               {
                  "desc": false,
                  "displayName": "Name"
               }
            ]
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "headscale_nodes_info{\n  cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n}\n",
               "format": "table",
               "instant": true
            }
         ],
         "title": "Nodes Inventory",
         "transformations": [
            {
               "id": "organize",
               "options": {
                  "excludeByName": {
                     "Time": true,
                     "Value": true,
                     "__name__": true,
                     "cluster": true,
                     "container": true,
                     "endpoint": true,
                     "environment": true,
                     "instance": true,
                     "job": true,
                     "namespace": true,
                     "pod": true,
                     "prometheus": true,
                     "service": true
                  },
                  "indexByName": {
                     "disco_key": 7,
                     "given_name": 3,
                     "id": 8,
                     "machine_key": 5,
                     "name": 0,
                     "node_key": 6,
                     "register_method": 4,
                     "user": 1,
                     "user_id": 2
                  },
                  "renameByName": {
                     "disco_key": "Disco Key",
                     "given_name": "Given Name",
                     "id": "ID",
                     "machine_key": "Machine Key",
                     "name": "Name",
                     "node_key": "Node Key",
                     "register_method": "Register Method",
                     "user": "User",
                     "user_id": "User ID"
                  }
               }
            }
         ],
         "type": "table"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Creation, last seen, and expiry timestamps per node.",
         "fieldConfig": {
            "defaults": {
               "unit": "string"
            },
            "overrides": [
               {
                  "matcher": {
                     "id": "byName",
                     "options": "Created"
                  },
                  "properties": [
                     {
                        "id": "unit",
                        "value": "dateTimeAsIso"
                     }
                  ]
               },
               {
                  "matcher": {
                     "id": "byName",
                     "options": "Last Seen"
                  },
                  "properties": [
                     {
                        "id": "unit",
                        "value": "dateTimeAsIso"
                     }
                  ]
               },
               {
                  "matcher": {
                     "id": "byName",
                     "options": "Expiry"
                  },
                  "properties": [
                     {
                        "id": "unit",
                        "value": "dateTimeAsIso"
                     }
                  ]
               }
            ]
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 30
         },
         "id": 18,
         "options": {
            "footer": {
               "enablePagination": true
            },
            "sortBy": [
               {
                  "desc": false,
                  "displayName": "Name"
               }
            ]
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "min(\n  headscale_nodes_created_timestamp{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  } * 1000\n) by (id, name, user)\n",
               "format": "table",
               "instant": true
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "max(\n  headscale_nodes_last_seen_timestamp{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  } * 1000\n) by (id, name, user)\n",
               "format": "table",
               "instant": true
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "min(\n  headscale_nodes_expiry_timestamp{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  } * 1000\n) by (id, name, user)\n",
               "format": "table",
               "instant": true
            }
         ],
         "title": "Node Lifecycle",
         "transformations": [
            {
               "id": "merge"
            },
            {
               "id": "organize",
               "options": {
                  "excludeByName": {
                     "Time": true,
                     "__name__": true,
                     "cluster": true,
                     "container": true,
                     "endpoint": true,
                     "environment": true,
                     "instance": true,
                     "job": true,
                     "namespace": true,
                     "pod": true,
                     "prometheus": true,
                     "service": true
                  },
                  "indexByName": {
                     "Value #A": 3,
                     "Value #B": 4,
                     "Value #C": 5,
                     "id": 1,
                     "name": 0,
                     "user": 2
                  },
                  "renameByName": {
                     "Value #A": "Created",
                     "Value #B": "Last Seen",
                     "Value #C": "Expiry",
                     "id": "ID",
                     "name": "Name",
                     "user": "User"
                  }
               }
            }
         ],
         "type": "table"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "How long each node has been offline according to Headscale.",
         "fieldConfig": {
            "defaults": {
               "unit": "s"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 30
         },
         "id": 19,
         "options": {
            "footer": {
               "enablePagination": true
            },
            "sortBy": [
               {
                  "desc": true,
                  "displayName": "Value"
               }
            ]
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "time() -\nmax(\n  headscale_nodes_last_seen_timestamp{\n    cluster=\"$cluster\"\n,\nnamespace=\"$namespace\",\njob=\"$job\"\n\n  }\n) by (id, name, user)\n",
               "format": "table",
               "instant": true
            }
         ],
         "title": "Seconds Since Last Seen",
         "transformations": [
            {
               "id": "organize",
               "options": {
                  "excludeByName": {
                     "Time": true,
