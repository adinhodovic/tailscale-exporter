      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "A table panel showing the top 20 Tailscale machines by inbound traffic over the last hour.",
         "fieldConfig": {
            "defaults": {
               "unit": "Bps"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 8,
            "w": 8,
            "x": 0,
            "y": 7
         },
         "id": 8,
         "options": {
            "footer": {
               "enablePagination": true
            },
            "sortBy": [
               {
                  "desc": true,
                  "displayName": "Inbound Traffic (Bps)"
               }
            ]
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "topk(\n  20,\n  sum(\n    rate(\n      tailscaled_inbound_bytes_total{\n        cluster=\"$cluster\"\n,\njob=~\"$job\"\n\n      }[1h]\n    )\n  ) by (tailscale_machine)\n)\n",
               "format": "table",
               "instant": true
            }
         ],
         "title": "Top 20 Machines by Inbound Traffic (1h)",
         "transformations": [
            {
               "id": "organize",
               "options": {
                  "excludeByName": {
                     "Time": true,
                     "job": true
                  },
                  "indexByName": {
                     "Value": 1,
                     "tailscale_machine": 0
                  },
                  "renameByName": {
                     "Value": "Inbound Traffic (Bps)",
                     "tailscale_machine": "Tailscale Machine"
                  }
               }
            }
         ],
         "type": "table"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "A table panel showing the Tailscale machines with unadvertised routes.",
